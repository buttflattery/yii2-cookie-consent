{
  "name": "dmstr/yii2-cookie-consent",
  "description": "Yii2 Cookie Consent Widget",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "cookie consent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "German Bisurgi",
      "email": "g.bisurgi@herzogkommunikation.de"
    }
  ],
  "require": {
    "fxp/composer-asset-plugin": "^1.2.0",
    "yiisoft/yii2": "~2.0.0",
    "npm-asset/dmstr--cookie-consent": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "dmstr\\cookieconsent\\": "./src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
